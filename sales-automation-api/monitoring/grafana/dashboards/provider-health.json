      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 },
      "id": 6,
      "options": { "legend": { "calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom" } },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(provider_api_latency_ms_bucket[5m])) by (provider, le))",
